{
    "manifest_version": 2,
    "name": "dicto",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_popup": "pad.html",
        "default_title": "Defination is: "
    }

}